import java.io.*;
import java.util.*;
public class Main
{
	public static void main(String args[])
	{
		Scanner s=new Scanner(System.in);
		int n;
		n=s.nextInt();
		if(n==0)
		{
			System.out.print("Zero");
		}
		else
		{
			if(n<0)
			{
				System.out.print("Negative");
			}
			else
			{
				System.out.print("Positive");	
			}
		}
	}
}
